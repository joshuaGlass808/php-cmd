{
    "name": "scf/simple-command-factory",
    "type": "project",
    "homepage": "https://github.com/joshuaGlass808/simple-command-factory",
    "description": "The Simple Command Factory.",
    "license": "MIT",
    "keywords": [
        "framework",
        "commands",
        "php-cmd",
        "scf"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "SCF\\": "src/"
        }
    },
    "require": {
        "php": "^7.4"
    },
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    }
}
